{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "BBTestDataFactory"
		},
		"ContainerSFTP_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ContainerSFTP'"
		},
		"Input_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Input'"
		},
		"Input2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Input2'"
		},
		"PomeloSQL_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'PomeloSQL'"
		},
		"Sftp1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Sftp1'"
		},
		"SqlProd_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlProd'"
		},
		"TIDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'TIDB'"
		},
		"Sftp1_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "foreducation.files.com"
		},
		"Sftp1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "jutha"
		},
		"Auto-onCreate_properties_CSVToSQL_parameters_filename": {
			"type": "string",
			"defaultValue": "@triggerBody().filename"
		},
		"Auto-onCreate_properties_CatalogueCSVToSQL_parameters_filename": {
			"type": "string",
			"defaultValue": "@triggerBody().filename"
		},
		"Auto-onCreate_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/d99b4590-2f3a-42b3-aa04-8c6ffbc7ecf2/resourceGroups/BBTestDataFactory/providers/Microsoft.Storage/storageAccounts/bbtestdatafactory2"
		},
		"trigger2_properties_CustomFieldCSVToSQL_parameters_filename": {
			"type": "string",
			"defaultValue": "@triggerBody().filename"
		},
		"trigger2_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/d99b4590-2f3a-42b3-aa04-8c6ffbc7ecf2/resourceGroups/BBTestDataFactory/providers/Microsoft.Storage/storageAccounts/bbtestdatafactory"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "BBTestDataFactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ContainerSFTP_connectionString": {
						"value": "[parameters('ContainerSFTP_connectionString')]"
					},
					"Input_connectionString": {
						"value": "[parameters('Input_connectionString')]"
					},
					"Input2_connectionString": {
						"value": "[parameters('Input2_connectionString')]"
					},
					"PomeloSQL_connectionString": {
						"value": "[parameters('PomeloSQL_connectionString')]"
					},
					"Sftp1_password": {
						"value": "[parameters('Sftp1_password')]"
					},
					"TIDB_connectionString": {
						"value": "[parameters('TIDB_connectionString')]"
					},
					"SqlProd_connectionString": {
						"value": "[parameters('SqlProd_connectionString')]"
					},
					"Sftp1_properties_typeProperties_host": {
						"value": "[parameters('Sftp1_properties_typeProperties_host')]"
					},
					"Sftp1_properties_typeProperties_userName": {
						"value": "[parameters('Sftp1_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "BBTestDataFactory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Auto-onCreate_properties_CSVToSQL_parameters_filename": {
						"value": "[parameters('Auto-onCreate_properties_CSVToSQL_parameters_filename')]"
					},
					"Auto-onCreate_properties_CatalogueCSVToSQL_parameters_filename": {
						"value": "[parameters('Auto-onCreate_properties_CatalogueCSVToSQL_parameters_filename')]"
					},
					"Auto-onCreate_properties_typeProperties_scope": {
						"value": "[parameters('Auto-onCreate_properties_typeProperties_scope')]"
					},
					"trigger2_properties_CustomFieldCSVToSQL_parameters_filename": {
						"value": "[parameters('trigger2_properties_CustomFieldCSVToSQL_parameters_filename')]"
					},
					"trigger2_properties_typeProperties_scope": {
						"value": "[parameters('trigger2_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'BBTestDataFactory_ArmTemplate_0')]"
			]
		}
	]
}