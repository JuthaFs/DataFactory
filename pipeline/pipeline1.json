									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "COMPL_DTE_TIMEZONE",
									"type": "String"
								},
								"sink": {
									"name": "COMPL_DTE_TIMEZONE",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "CREDIT_HRS",
