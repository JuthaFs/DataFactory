									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "INST_NAME",
									"type": "String"
								},
								"sink": {
									"name": "INST_NAME",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "COMMENTS",
									"type": "String"
								},
								"sink": {
